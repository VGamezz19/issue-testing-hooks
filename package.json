{
	"name": "engloba-cmms-web",
	"version": "0.1.0",
	"private": false,
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts --max_old_space_size=4096 build",
		"test": "react-scripts test --watchAll=false --transformIgnorePatterns \"camunda-bpmn-moddle\"",
		"lint": "eslint \"./src/**/*.{js,jsx}\""
	},
	"dependencies": {
		"@date-io/moment": "^1.3.13",
		"@engloba-tech/englobity": "0.0.57",
		"@known-as-bmf/react-signalr": "^1.0.6",
		"@material-ui/core": "^4.11.3",
		"@material-ui/icons": "^4.11.2",
		"@material-ui/lab": "^4.0.0-alpha.56",
		"@material-ui/pickers": "^3.2.10",
		"@microsoft/signalr": "^5.0.7",
		"@rjsf/core": "^2.5.0",
		"@rjsf/material-ui": "^2.5.0",
		"axios": "^0.21.1",
		"axios-extensions": "^3.1.3",
		"axios-middleware": "^0.3.1",
		"azure-storage": "^2.10.4",
		"bpmn-js": "8.3.1",
		"bpmn-js-properties-panel": "0.42.0",
		"camunda-bpmn-moddle": "^5.1.2",
		"clsx": "^1.1.1",
		"file-saver": "^2.0.5",
		"fontsource-roboto": "^3.0.3",
		"i18next": "^19.8.4",
		"i18next-browser-languagedetector": "^6.0.1",
		"jsonwebtoken": "^8.5.1",
		"material-table": "^1.63.0",
		"moment": "^2.29.1",
		"nprogress": "^0.2.0",
		"poppins-font": "^1.0.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-draggable": "^4.4.3",
		"react-dropzone": "^11.2.4",
		"react-i18next": "^11.7.3",
		"react-number-format": "^4.6.0",
		"react-router-dom": "^5.2.0",
		"react-scripts": "^4.0.3",
		"react-swipeable-views": "^0.13.9",
		"rxjs": "^7.2.0",
		"shortid": "^2.2.16",
		"testcafe-react-selectors": "^4.0.0",
		"uuid": "^8.3.2",
		"web-vitals": "^0.2.4"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest",
			"plugin:react/recommended"
		],
		"ignorePatterns": [
			"src/*",
			"!src/test.js"
		],
		"plugins": [
			"react-hooks"
		],
		"globals": {
			"fixture": true,
			"location": true
		},
		"rules": {
			"react-hooks/rules-of-hooks": "error",
			"react-hooks/exhaustive-deps": "warn",
			"no-throw-literal": "off",
			"react/jsx-fragments": "error",
			"react/prop-types": 1,
			"no-console": [
				"warn",
				{
					"allow": [
						"warn",
						"error"
					]
				}
			],
			"no-debugger": "warn",
			"no-use-before-define": 2,
			"spaced-comment": "warn",
			"quotes": [
				"error",
				"single"
			],
			"semi": [
				"error",
				"always"
			]
		}
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^5.14.1",
		"@testing-library/react": "^12.1.0",
		"@testing-library/react-hooks": "^7.0.2",
		"@types/jest": "^26.0.15",
		"@wojtekmaj/enzyme-adapter-react-17": "^0.3.1",
		"jest-transform-stub": "^2.0.0",
		"react-test-renderer": "^16.9.0"
	},
	"_id": "engloba-cmms-web@0.1.0"
}
